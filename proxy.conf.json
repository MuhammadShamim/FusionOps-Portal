{
  "/api": {
    "target": "AP_PROXY_URL",
    "secure": false,
    "changeOrigin": true,
    "logLevel": "debug"
  },
  "/pagerduty": {
    "target": "https://api.pagerduty.com",
    "secure": true,
    "changeOrigin": true,
    "pathRewrite": { "^/pagerduty": "" },
    "logLevel": "debug"
  },
  "/api/speedship/*": {
    "target": "https://www.speedship.com/",
    "secure": true,
    "changeOrigin": true,
    "logLevel": "debug",
    "pathRewrite": {
      "^/api/speedship": ""
    }
  }
}
